RewriteEngine on

RewriteRule ^1\.x/(.*)$ 1.2/$1 [L,env=nocache:true]
RewriteRule ^2\.x/(.*)$ 2.0/$1 [L,env=nocache:true]
RewriteRule ^3\.x/(.*)$ 3.1/$1 [L,env=nocache:true]
RewriteRule ^4\.x/(.*)$ 4.0/$1 [L,env=nocache:true]
RewriteRule ^latest/(.*)$ 3.x/$1 [L,env=nocache:true]

Header set Cache-Control no-cache env=REDIRECT_nocache
